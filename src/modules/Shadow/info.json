{
    "name": "Shadow",
    "description": "Generates a shadow behind the image",
    "inputs": {
        "x": {
            "type": "integer",
            "desc": "starting X-position of the shadow",
            "default": 0
        },
        "y": {
            "type": "integer",
            "desc": "starting Y-position of the shadow",
            "default": 0
        },
        "fillColor": {
            "type": "String",
            "desc": "four space separated numbers representing the RGBA values of fill-color",
            "default": "100 100 100 255",
            "placeholder": "100 100 100 255"
        }
    }
}
