{
    "name": "Draw Rectangle",
    "description": "It draws a rectangle on the image",
    "inputs": {
  		"startingX":{
        "type": "integer",
  			"desc": "starting x position of the rectangle",
        "htmlType":{
          "type": "number",
          "default": 0
        }
  		},
        
      "startingY": {
        "type": "integer",
        "desc": "starting y position of the rectangle",
        "htmlType":{
          "type": "number",
          "default": 0
        }
      },

      "endX":{
        "type": "integer",
        "desc": "last x position of the rectangle",
        "htmlType":{
          "type": "number",
          "default": 0
        }
      },

      "endY":{
        "type": "integer",
        "desc": "last y position of the rectangle",
        "htmlType":{
          "type": "number",
          "default": 0
        }
      },

      "thickness":{
        "type": "integer",
        "desc": "thickness of border",
        "htmlType":{
          "type": "number",
          "default": 1
        }
      },

      "color":{
        "type": "string",
        "desc": "RGBA values separated by a space",
        "htmlType":
        {
          "type": "text",
          "default": "0 0 0 255"
        }
      }
    }
}
