{
    "name": "gradient",
    "description": "Gives a gradient for the image",
    "inputs": {
      "gradientType": {
        "type": "select",
        "desc": "Choose an algorithm to follow",
        "default": "linear(left)",
        "values": ["linear(left)", "linear(bottom)",  "circular"]
      },
      "width": {
        "type": "string",
        "desc": "Final width of the canvas",
        "default": "100%"
      },
      "height": {
          "type": "string",
          "desc": "Final height of the canvas",
          "default": "100%"
      },
      "startingColor": {
        "type": "text",
        "desc": "Starting Color",
        "default": "rgba(0,0,0,255)",
        "id": "color-picker"
      },
      "endingColor": {
        "type": "text",
        "desc": "Ending Color",
        "default": "rgba(255,255,255,255)",
        "id": "color-picker"
      }
    },
    "docs-link":"https://github.com/publiclab/image-sequencer/blob/main/docs/MODULES.md#gradient-module"
}
